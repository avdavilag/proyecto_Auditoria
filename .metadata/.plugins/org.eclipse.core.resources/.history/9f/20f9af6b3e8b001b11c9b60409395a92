package Examencomidaplatos.model.managers;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

import javax.ejb.EJB;
import javax.enterprise.context.SessionScoped;
import javax.enterprise.inject.Alternative;
import javax.inject.Named;
import javax.persistence.EntityManager;
import javax.persistence.PersistenceContext;

import Examencomidaplatos.model.dto.dtoplatoscomida;
import Examencomidasplatos.model.entities.Platoscomida;




@Alternative
@Named
@SessionScoped
public class ManagersExamencomidaplatos implements Serializable {

	public ManagersExamencomidaplatos() {
		// TODO Auto-generated constructor stub
	}
	@PersistenceContext
	private EntityManager em;
	public List<dtoplatoscomida> findAllUsuarios() {
		return em.createNamedQuery("ComprasUsuario.findAll", dtoplatoscomida.class).getResultList();
	}
}
